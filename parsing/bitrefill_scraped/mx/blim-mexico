const dolog=false;
let fetchStart=0;
if(performance && performance.getEntriesByType) fetchStart=performance.getEntriesByType("navigation")[0].fetchStart;
const beaconLog={};
let ended=0;
const beacon_url0=document.location.href;
const beacon_ref0=document.referrer;

function getInfo()
{
  const q0=performance.getEntriesByType("navigation")[0];
  const st=q0.responseStart;
  const en=q0.responseEnd;
  const transferTime=en-st;
  const size=q0.transferSize||q0.encodedBodySize||q0.decodedBodySize;
  const bps=((size*8)/(transferTime/1000));
  return {transferTime,size,bps,st,en};
}


function beacon(k,v)
{
  const t=performance.now()-fetchStart;
  if(!beaconLog[k])
  {
    if(dolog) console.log('beacon',t,k,v);
    beaconLog[k]={t,v};
  }
}
window.addEventListener("error",e=>{
  let msg='';
  let stack=[];
  if(e.message) msg=e.message;
  if(e.error)
  {
  if(e.error.stack) stack=e.error.stack.split(/\n\s+/);
  if(e.error.message) msg=e.error.message;
  }
  beacon('error',{ msg,stack } ) ;
});

for(const ev of['load','DOMContentLoaded','scroll']) window.addEventListener(ev,e=>{beacon(ev);});
window.addEventListener('DOMContentLoaded',()=>{
  if(dolog) console.log('beacon',{fetchStart},getInfo());
});
for(const ev of['pagehide','beforeunload'])
window.addEventListener(ev,e=>{
if(!ended++){
beacon('end');
const beacon_url1=document.location.href;
const beacon_ref1=document.referrer;
const i=getInfo();
const debug0=new Date(1970,8,1,13).toLocaleString();
const debug1=new Date(1970,8,1,13).toString();

navigator.sendBeacon("/api/beacon", JSON.stringify({version:2,
  brid:window.__brid,
  isfirst:window.__isfirst,
  fetchStart,
  bps:i.bps,
  transferTime:i.transferTime,
  sessionLength:performance.now(),
  al0:navigator.languages,
  al1:window.__accept_language,
  debug0,
  debug1,
  url0:beacon_url0,
  url1:beacon_url1,
  ref:beacon_ref0,
  log:beaconLog}));
}
});

window.__beacon=beacon;
beacon('start');

        </script>
